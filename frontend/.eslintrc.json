{
  "extends": ["next", "next/core-web-vitals"],
  "rules": {
    "@next/next/no-html-link-for-pages": "off",
    "no-console": ["warn", {"allow": ["warn", "error"]}],
    "@typescript-eslint/no-explicit-any": "warn",
    "eqeqeq": ["error", "always"],
    "curly": ["error", "all"],
    "no-unused-vars": ["warn", {"argsIgnorePattern": "^_"}]
  }
}
